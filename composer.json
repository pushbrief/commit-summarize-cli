{
    "name": "pushbrief/pre-commit-summarize",
    "type": "package",
    "require": {
        "symfony/console": "^7.2",
        "symfony/process": "^7.2",
        "lesstif/php-jira-rest-client": "^5.9",
        "vlucas/phpdotenv": "^5.5",
        "openai-php/client": "^0.10.3",
        "symfony/http-client": "^7.2",
        "nyholm/psr7": "^1.8"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Pushbrief\\PreCommitSummarize\\": "src/"
        }
    },
    "bin": [
        "bin/pre-commit-summarize"
    ],
    "authors": [
        {
            "name": "Ahmet",
            "email": "ahmetbarut588@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.11",
        "symfony/var-dumper": "^7.2"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
